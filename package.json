{
  "name": "iterable-transform-replace",
  "version": "1.1.1",
  "description": "An iterable transform that replace specified item",
  "scripts": {
    "test": "ava && xo --ignore example.js --ignore index.js",
    "setup": "rollup -f cjs main.js --output index.js",
    "cover-test": "nyc ava && xo --ignore example.js --ignore index.js",
    "cover-publish": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": ["iterable", "replace", "functional", "curry"],
  "author": "Andrea Parodi <andrea@parro.it>",
  "license": "MIT",
  "files": [
    "index.js",
    "example.js"
  ],
  "tonicExampleFilename": "example.js",
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "jsnext:main": "main.js",
  "repository": "parro-it/iterable-transform-replace",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.14",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "curry": "^1.2.0"
  }
}
